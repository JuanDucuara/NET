@model NET.Models.UsuarioIndex

@{
    ViewBag.Title = "Paginador-Index";
}

<h2>Paginador Index</h2>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Email</th>
        </tr>
    </thead>
   
    @foreach (var usuario in Model.Usuarios)
    {
<tbody>
    <tr>
        <td>@usuario.nombre</td>
        <td>@usuario.apellido</td>
        <td>@usuario.email</td>
    </tr>
</tbody>
   
    }
</table>

@{ Html.RenderPartial("Paginador", Model); }